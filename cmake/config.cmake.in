include(FindPackageHandleStandardArgs)
set(${CMAKE_FIND_PACKAGE_NAME}_CONFIG ${CMAKE_CURRENT_LIST_FILE})
find_package_handle_standard_args(@PROJECT_NAME@ CONFIG_MODE)

if(NOT TARGET @PROJECT_NAME@::@PERFVECT_TARGET_NAME@)
	include("${CMAKE_CURRENT_LIST_DIR}/@PERFVECT_TARGETS_EXPORT_NAME@.cmake")
	if((NOT TARGET @PERFVECT_TARGET_NAME@) AND (NOT @PROJECT_NAME@_FIND_VERSION))
		add_library(@PERFVECT_TARGET_NAME@ INTERFACE IMPORTED)
		set_target_properties(@PERFVECT_TARGET_NAME@ PROPERTIES
			INTERFACE_LINK_LIBRARIES @PROJECT_NAME@::@PERFVECT_TARGET_NAME@
		)
	endif()
endif()